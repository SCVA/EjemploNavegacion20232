<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="moviles.ejemplonavegacion.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_unoFragment"
            app:destination="@id/unoFragment" />
        <action
            android:id="@+id/action_mainFragment_to_dosFragment"
            app:destination="@id/dosFragment" />
    </fragment>
    <fragment
        android:id="@+id/unoFragment"
        android:name="moviles.ejemplonavegacion.UnoFragment"
        android:label="fragment_uno"
        tools:layout="@layout/fragment_uno" />
    <fragment
        android:id="@+id/dosFragment"
        android:name="moviles.ejemplonavegacion.DosFragment"
        android:label="fragment_dos"
        tools:layout="@layout/fragment_dos" />
</navigation>